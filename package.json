  "license": "ISC",
  "description": "This website is made by me to practice using tailwind css. And it's not for sale.",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "live-server": "^1.2.2",
    "tailwindcss": "^3.4.15"
  }
}